
*{
  margin:0px;
  padding:0px;
  color : #000;
}
ul,li{
  list-style:none;
}
a{
  color:inherit;
  text-decoration:none;
}
#allRound{
	width : 90%;
	height : 100%;
	margin-left : auto;
	margin-right : auto;
}
#subtitle{
	background : #001353;
	color : #fff;
	display : inline-block;
	width : 50%;
	text-align : center;
	padding : 15px;
	border-radius : 10px 10px 0px 0px;
	cursor : pointer;
}
#subtitle2{
	display : inline-block;
	background : #fff;
	float : right;
	width : 50%;
	text-align : center;
	padding : 15px;
	border-radius : 10px 10px 0px 0px;
	cursor : pointer;
}
#image_container{
	width : 115px;
	height : 145px;
	border: 1px solid #777;
	border-radius : 10px;
}
#uploadForm{
	display : inline-block;
	position : relative;
	left : 130px;
	bottom : 40px;
}
#profile{
	display : inline-block;
	width : 100%;
	height : 100%;
	border-radius : 10px;
}
.profileImg{
	width : 115px;
	height : 145px;
	position :relative;
	bottom : 145px;
	border-radius : 10px;
}
#saveProfile{
	background : #2a5388;
	box-shadow : #2a5388;
	border : #2a5388;
	position : relative;
	bottom : 4px;
	left : 3px;
}
#saveProfile:hover{
	background : #4371af;
	box-shadow : #4371af;
	border : #4371af;
}
#updateInfo label{
	display : inline-block;
	width : 100px;
	font-size : 12pt;
	padding-left : 15px;
}
#updateInfo input{
	text-align : center;
	width : 200px;
}
#memBank{
	width : 200px;
	text-align : center;
}
#memBank, #updateInfo input{
	padding : 5px;
	border-radius : 5px;
	border : 1px solid #aaa;
}
#updateInfo{
	padding-left : 20px;
}
#updateInfo label{
	margin : 5px;
}

#updateInfo label,#updateInfo input, #memBank{
	margin-top : 10px;
}

#changePw{
	display : none;
}
#tgradeYellowBox {
    background: lightyellow;
    border: 1px solid lightgray;
    margin: 10px;
    padding: 10px;
}
#tgradeGreenText {
    color: green;
}
#changePw input[type=password]{
	width : 40%;
	height : 40px;
	margin-top : 10px;
	border-radius : 5px;
	border:1px solid #aaa;
	padding : 5px;
}

#changePwBtn{
	background:#2a5388;
	box-shadow:#2a5388;
	border:#2a5388;
}

#changePwBtn:hover{
	background:#4671af;
	box-shadow:#4671af;
	border:#4671af;
}

#changePwBtn{
	margin-top : 20px;
	margin-left : 315px;
}