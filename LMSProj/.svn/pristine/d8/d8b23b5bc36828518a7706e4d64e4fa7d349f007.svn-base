package kr.ac.lms.vo;

import org.apache.commons.lang.builder.ToStringBuilder;
import org.apache.commons.lang.builder.ToStringStyle;

public class TestDetailVO {
	private int tdCd;
	private int teqCd;
	private int stCd;
	private int tdAnswer;
	public int getTdCd() {
		return tdCd;
	}
	public void setTdCd(int tdCd) {
		this.tdCd = tdCd;
	}
	public int getTeqCd() {
		return teqCd;
	}
	public void setTeqCd(int teqCd) {
		this.teqCd = teqCd;
	}
	public int getStCd() {
		return stCd;
	}
	public void setStCd(int stCd) {
		this.stCd = stCd;
	}
	public int getTdAnswer() {
		return tdAnswer;
	}
	public void setTdAnswer(int tdAnswer) {
		this.tdAnswer = tdAnswer;
	}

	@Override
	public String toString() {
		return ToStringBuilder.reflectionToString(this, ToStringStyle.DEFAULT_STYLE);
	}
}
