package kr.ac.lms.vo;

import java.util.Date;

import org.apache.commons.lang.builder.ToStringBuilder;
import org.apache.commons.lang.builder.ToStringStyle;

public class AttenadenceVO {
	private int atenCd;
	private int lecCd;
	private int stuCd;
	private Date atenCate;
	private String atenDt;
	
	public int getAtenCd() {
		return atenCd;
	}
	public void setAtenCd(int atenCd) {
		this.atenCd = atenCd;
	}
	public int getLecCd() {
		return lecCd;
	}
	public void setLecCd(int lecCd) {
		this.lecCd = lecCd;
	}
	public int getStuCd() {
		return stuCd;
	}
	public void setStuCd(int stuCd) {
		this.stuCd = stuCd;
	}
	public Date getAtenCate() {
		return atenCate;
	}
	public void setAtenCate(Date atenCate) {
		this.atenCate = atenCate;
	}
	public String getAtenDt() {
		return atenDt;
	}
	public void setAtenDt(String atenDt) {
		this.atenDt = atenDt;
	}

	@Override
	public String toString() {
		return ToStringBuilder.reflectionToString(this, ToStringStyle.DEFAULT_STYLE);
	}
	
}
