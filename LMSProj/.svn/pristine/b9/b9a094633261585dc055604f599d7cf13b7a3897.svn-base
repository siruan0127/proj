package kr.ac.lms.vo;

import java.util.Date;
import java.util.List;

import org.apache.commons.lang.builder.ToStringBuilder;
import org.apache.commons.lang.builder.ToStringStyle;

public class CommonVO {

	private String comCd;
	private String comNm;
	private String comDetail;
	private Date comReg;
	private int comMem;
	private Date comUpd;
	private int comUpcd;
	
	private List<CommonDetailVO> comdDetailList;
	
	public List<CommonDetailVO> getComdDetailList() {
		return comdDetailList;
	}
	public void setComdDetailList(List<CommonDetailVO> comdDetailList) {
		this.comdDetailList = comdDetailList;
	}
	public String getComCd() {
		return comCd;
	}
	public void setComCd(String comCd) {
		this.comCd = comCd;
	}
	public String getComNm() {
		return comNm;
	}
	public void setComNm(String comNm) {
		this.comNm = comNm;
	}
	public String getComDetail() {
		return comDetail;
	}
	public void setComDetail(String comDetail) {
		this.comDetail = comDetail;
	}
	public Date getComReg() {
		return comReg;
	}
	public void setComReg(Date comReg) {
		this.comReg = comReg;
	}
	public int getComMem() {
		return comMem;
	}
	public void setComMem(int comMem) {
		this.comMem = comMem;
	}
	public Date getComUpd() {
		return comUpd;
	}
	public void setComUpd(Date comUpd) {
		this.comUpd = comUpd;
	}
	public int getComUpcd() {
		return comUpcd;
	}
	public void setComUpcd(int comUpcd) {
		this.comUpcd = comUpcd;
	}
	
	@Override
	public String toString() {
		return ToStringBuilder.reflectionToString(this, ToStringStyle.DEFAULT_STYLE);
	}
}
