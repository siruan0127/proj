package kr.ac.lms.vo;

import org.apache.commons.lang.builder.ToStringBuilder;
import org.apache.commons.lang.builder.ToStringStyle;

public class ScholarshipVO {
	private int sclCd;
	private String sclNm;
	private int sclAmt;
	private String sclDetail;
	private int sclMax;
	public int getSclCd() {
		return sclCd;
	}
	public void setSclCd(int sclCd) {
		this.sclCd = sclCd;
	}
	public String getSclNm() {
		return sclNm;
	}
	public void setSclNm(String sclNm) {
		this.sclNm = sclNm;
	}
	public int getSclAmt() {
		return sclAmt;
	}
	public void setSclAmt(int sclAmt) {
		this.sclAmt = sclAmt;
	}
	public String getSclDetail() {
		return sclDetail;
	}
	public void setSclDetail(String sclDetail) {
		this.sclDetail = sclDetail;
	}
	public int getSclMax() {
		return sclMax;
	}
	public void setSclMax(int sclMax) {
		this.sclMax = sclMax;
	}

	@Override
	public String toString() {
		return ToStringBuilder.reflectionToString(this, ToStringStyle.DEFAULT_STYLE);
	}
}
