package kr.ac.lms.vo;

import org.apache.commons.lang.builder.ToStringBuilder;
import org.apache.commons.lang.builder.ToStringStyle;

public class ReservationVO {
	private int rsvCd;
	private int facCd;
	private int memCd;
	private String rsvSt;
	private String rsvEn;
	private String memNm;
	
	public int getRsvCd() {
		return rsvCd;
	}
	public void setRsvCd(int rsvCd) {
		this.rsvCd = rsvCd;
	}
	public int getFacCd() {
		return facCd;
	}
	public void setFacCd(int facCd) {
		this.facCd = facCd;
	}
	public int getMemCd() {
		return memCd;
	}
	public void setMemCd(int memCd) {
		this.memCd = memCd;
	}
	public String getRsvSt() {
		return rsvSt;
	}
	public void setRsvSt(String rsvSt) {
		this.rsvSt = rsvSt;
	}
	public String getRsvEn() {
		return rsvEn;
	}
	public void setRsvEn(String rsvEn) {
		this.rsvEn = rsvEn;
	}
	public String getMemNm() {
		return memNm;
	}
	public void setMemNm(String memNm) {
		this.memNm = memNm;
	}
	
	@Override
	public String toString() {
		return ToStringBuilder.reflectionToString(this, ToStringStyle.DEFAULT_STYLE);
	}
}