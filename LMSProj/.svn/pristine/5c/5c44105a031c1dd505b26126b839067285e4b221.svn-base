@charset "UTF-8";

*{
  margin:0;
  padding:0;
}
ul,li{
  list-style:none;
}
a{
  color:inherit;
  text-decoration:none;
}

.imgSlide{
	overflow : hidden;
	width: 100%;
	height : 100%;
}
.slides, .slides li{
	display : inline-block;
	width : 100%;
	height : 100%;
	position : absolute;
	top: 0;
	left : 0;
	overflow : hidden;
}
.slides #roll{
	display : inline-block;
	width : 100%;
	height : 100%;
	overflow : hidden;
}
#allRound{
	position: relative;
	margin-left : auto;
	margin-right : auto;
	margin-top : 315px;
	width : 800px;
	height : 350px;
	overflow:hidden;
}
.container{
	width : 100%;
	height : 90%;
	background : white;
	opacity : 70%;
	position : relative;
	margin-left:auto;
	margin-right : auto;
	padding : 20px;
	border-radius : 10px;
	border: 3px solid #001353;
}
#frm{
	margin-left : auto;
	margin-right : auto;
	width : 700px;
	position : relative;
	bottom : 71%;
}
.inputInfo{
	position : relative;
	left : 70px;
	top : 25px;
	width : 70%;
}
.loginLabel{
	font-size : 15pt;
	width : 100px;
	color : black;
	text-align : right;
	margin-left : 10px;
	margin-right : 30px;
	font-weight : normal;
}

#memCd,#memPass{
	display : inline-block;
	margin-top: 15px;
	width : 250px;
	height : 42px;
	border-radius : 5px;
	border : 1px solid #000;
	background : none;
	color : black;
}
#loginBottom{
	margin-top : 35px;
	margin-left : 210px;
	font-size : 1.1em;
	color : black;
	letter-spacing : 1.5px;
}
#findInfo:hover{
	color : black;
	text-decoration : underline;
}
#loginBtn{
	display : inline-block;
	width : 100px;
	height : 100px;
	border-color : #fff;
	border-radius : 10px;
	background : #001353;
	color : white;
	font-weight : bold;
	position : absolute;
	right : 100px;
	bottom : 103px;
	opacity : 90%;
}
#loginBtn:hover{
	background : #4671af;
}
#alertCapsLock {
	 color : red;
	 display : block;
	 width : 220px;
	 height : 20px;
	 margin-left : 20px;
	 margin-bottom : 20px;
}