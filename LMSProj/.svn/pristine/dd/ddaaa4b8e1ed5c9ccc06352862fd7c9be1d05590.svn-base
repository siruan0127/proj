package kr.ac.lms.vo;

import java.util.Date;
import java.util.List;

import org.apache.commons.lang.builder.ToStringBuilder;
import org.apache.commons.lang.builder.ToStringStyle;

public class ProfessorVO {
	private int proCd;
	private int depCd;
		private String depNm;
	private Date proJoin;
	private Date proRet;
	private String proPos;
		private String proPosNm;
	
	private DepartmentVO departmentVO;
	private List<CommonVO> proPosList;
	
	
	
	public String getProPosNm() {
		return proPosNm;
	}
	public void setProPosNm(String proPosNm) {
		this.proPosNm = proPosNm;
	}
	public int getProCd() {
		return proCd;
	}
	public void setProCd(int proCd) {
		this.proCd = proCd;
	}
	public int getDepCd() {
		return depCd;
	}
	public void setDepCd(int depCd) {
		this.depCd = depCd;
	}
	public String getDepNm() {
		return depNm;
	}
	public void setDepNm(String depNm) {
		this.depNm = depNm;
	}
	public Date getProJoin() {
		return proJoin;
	}
	public void setProJoin(Date proJoin) {
		this.proJoin = proJoin;
	}
	public Date getProRet() {
		return proRet;
	}
	public void setProRet(Date proRet) {
		this.proRet = proRet;
	}
	public String getProPos() {
		return proPos;
	}
	public void setProPos(String proPos) {
		this.proPos = proPos;
	}
	public DepartmentVO getDepartmentVO() {
		return departmentVO;
	}
	public void setDepartmentVO(DepartmentVO departmentVO) {
		this.departmentVO = departmentVO;
	}
	public List<CommonVO> getProPosList() {
		return proPosList;
	}
	public void setProPosList(List<CommonVO> proPosList) {
		this.proPosList = proPosList;
	}

	@Override
	public String toString() {
		return ToStringBuilder.reflectionToString(this, ToStringStyle.DEFAULT_STYLE);
	}
}
