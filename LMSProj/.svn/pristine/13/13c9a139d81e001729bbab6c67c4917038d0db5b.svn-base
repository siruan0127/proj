@charset "UTF-8";

*{
  margin:0;
  padding:0;
}
ul,li{
  list-style:none;
}
a{
  color:inherit;
  text-decoration:none;
}

.imgSlide{
	overflow : hidden;
	width: 100%;
	height : 100%;
}
.slides, .slides li{
	display : inline-block;
	width : 100%;
	height : 100%;
	position : absolute;
	top: 0;
	left : 0;
	overflow : hidden;
}
.slides #roll{
	display : inline-block;
	width : 100%;
	height : 100%;
	overflow : hidden;
}
#allRound{
	position: relative;
	margin-left : auto;
	margin-right : auto;
	margin-top : 280px;
	width : 800px;
	height : 500px;
	overflow:hidden;
}
.container{
	width : 100%;
	height : 100%;
	background : black;
	opacity : 40%;
}
#frm{
	margin-left : auto;
	margin-right : auto;
	width : 700px;
	position : relative;
	bottom : 330px;
}
.inputInfo{
	position : relative;
	left : 70px;
	top : 25px;
	width : 70%;
}
.loginLabel{
	font-size : 15pt;
	width : 100px;
	color : white;
	text-align : right;
	margin-right : 50px;
}

#memCd,#memPass{
	display : inline-block;
	color : white;
	font-weight : bold;
	background : black;
	opacity :80%;
	margin-top: 15px;
	width : 250px;
	height : 42px;
	border:none;
	border-bottom : 2px solid white;
}
#loginBottom{
	margin-top : 35px;
	margin-left : 210px;
	font-size : 1.1em;
	color : white;
	letter-spacing : 1.5px;
}
#findInfo:hover{
	color : white;
	text-decoration : underline;
}
#loginBtn{
	display : inline-block;
	width : 100px;
	height : 100px;
	border-color : #fff;
	border-radius : 10px;
	background : #000;
	color : white;
	font-weight : bold;
	position : absolute;
	right : 80px;
	bottom : 40px;
	opacity : 90%;
}