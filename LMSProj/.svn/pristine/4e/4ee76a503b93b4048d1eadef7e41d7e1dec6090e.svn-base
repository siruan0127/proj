package kr.ac.lms.vo;

public class StuLecVO {
	private int lecCd;
	private int stuCd;
	private int slScore;
	public int getLecCd() {
		return lecCd;
	}
	public void setLecCd(int lecCd) {
		this.lecCd = lecCd;
	}
	public int getStuCd() {
		return stuCd;
	}
	public void setStuCd(int stuCd) {
		this.stuCd = stuCd;
	}
	public int getSlScore() {
		return slScore;
	}
	public void setSlScore(int slScore) {
		this.slScore = slScore;
	}
	@Override
	public String toString() {
		return "StuLecVO [lecCd=" + lecCd + ", stuCd=" + stuCd + ", slScore=" + slScore + "]";
	}
	
	
}
